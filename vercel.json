{
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "https://multi-auth-test-playaround.onrender.com/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://multi-auth-test-playaround.onrender.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
